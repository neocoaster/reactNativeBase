    "react-native-permissions": "^2.0.3",
    "react-native-reanimated": "^1.3.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
